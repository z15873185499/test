<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>head</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>HeadServlet</display-name>
    <servlet-name>HeadServlet</servlet-name>
    <servlet-class>servlet.HeadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HeadServlet</servlet-name>
    <url-pattern>/uploadimg</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>DeleteImgServlet</display-name>
    <servlet-name>DeleteImgServlet</servlet-name>
    <servlet-class>servlet.DeleteImgServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteImgServlet</servlet-name>
    <url-pattern>/dimg</url-pattern>
  </servlet-mapping>
</web-app>